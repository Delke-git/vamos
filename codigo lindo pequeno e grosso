								r = "XEQUE";
								xm++;

							} else if (!tab[i - n1][j + n1].equals("X") && !tab[i - n1][j + n1].equals("-")
									&& !tab[i - n1][j + n1].equals("q")) {
								r = "NAO XEQUE";
								break;
							} else if (tab[i - n1][j + n1].equals("-")) {
								r = "NAO XEQUE";
							}
							n1++;
						}
						n1 = 0;
						while ((i - n1) > 0 && (j - n1) > 0) {
							if (tab[i - n1][j - n1].equals("X") || tab[i - n1][j - n1].equals("K")) {
								r = "XEQUE";
								xm++;

							} else if (!tab[i - n1][j - n1].equals("X") && !tab[i - n1][j - n1].equals("-")
									&& !tab[i - n1][j - n1].equals("q")) {
								r = "NAO XEQUE";
								break;
							} else if (tab[i - n1][j - n1].equals("-")) {
								r = "NAO XEQUE";
							}
							n1++;
						}
						n1 = 0;
						while ((i + n1) < 8 && (j - n1) > 0) {
							if (tab[i + n1][j - n1].equals("X") || tab[i + n1][j - n1].equals("K")) {
								r = "XEQUE";
								xm++;

							} else if (!tab[i + n1][j - n1].equals("X") && !tab[i + n1][j - n1].equals("-")
									&& !tab[i + n1][j - n1].equals("q")) {
								r = "NAO XEQUE";
								break;
							} else if (tab[i + n1][j - n1].equals("-")) {
								r = "NAO XEQUE";
							}
							n1++;
						}
						n1 = 0;
						while ((i + n1) < 8 && (j + n1) < 8) {
							if (tab[i + n1][j + n1].equals("X") || tab[i + n1][j + n1].equals("K")) {
								r = "XEQUE";
								xm++;

							} else if (!tab[i + n1][j + n1].equals("X") && !tab[i + n1][j + n1].equals("-")
									&& !tab[i + n1][j + n1].equals("q")) {
								r = "NAO XEQUE";
								break;
							} else if (tab[i + n1][j + n1].equals("-")) {
								r = "NAO XEQUE";
							}
							n1++;
						}
						n1 = 0;
						while ((i - x1) > 0) {
							if (tab[i - x1][j].equals("X") || tab[i - x1][j].equals("K")) {
								r = "XEQUE";
								xm++;

							} else if (!tab[i - x1][j].equals("X") && !tab[i - x1][j].equals("-")
									&& !tab[i - x1][j].equals("q")) {
								r = "NAO XEQUE";
								break;
							} else if (tab[i - x1][j].equals("-")) {
								r = "NAO XEQUE";
							}
							x1++;
						}
						x1 = 0;
						while ((i + x1) < 8) {
							if (tab[i + x1][j].equals("X") || tab[i + x1][j].equals("K")) {
								r = "XEQUE";
								xm++;

							} else if (!tab[i + x1][j].equals("X") && !tab[i + x1][j].equals("-")
									&& !tab[i + x1][j].equals("q")) {
								r = "NAO XEQUE";
								break;
							} else if (tab[i + x1][j].equals("-")) {
								r = "NAO XEQUE";
							}
							x1++;
						}
						x1 = 0;
						while ((j - x1) > 0) {
							if (tab[i][j - x1].equals("X") || tab[i][j - x1].equals("K")) {
								r = "XEQUE";
								xm++;

							} else if (!tab[i][j - x1].equals("X") && !tab[i][j - x1].equals("-")
									&& !tab[i][j - x1].equals("q")) {
								r = "NAO XEQUE";
								break;
							} else if (tab[i][j - x1].equals("-")) {
								r = "NAO XEQUE";
							}
							x1++;
						}
						x = 0;
						while ((j + x) < 8) {
							if (tab[i][j + x].equals("X") || tab[i][j + x].equals("K")) {
								r = "XEQUE";
								xm++;

							} else if (!tab[i][j + x].equals("X") && !tab[i][j + x].equals("-")
									&& !tab[i][j + x].equals("q")) {
								r = "NAO XEQUE";
								break;
							} else if (tab[i][j + x].equals("-")) {
								r = "NAO XEQUE";
							}
							x++;
						}
						x = 0;
						break;
					case "c":
						if (i > 1 && j < 7) {
							if (tab[i - 2][j + 1].equals("X") || tab[i - 2][j + 1].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (i > 1 && j > 0) {
							if (tab[i - 2][j - 1].equals("X") || tab[i - 2][j - 1].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (i > 0 && j < 6) {
							if (tab[i - 1][j + 2].equals("X") || tab[i - 1][j + 2].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (i > 0 && j > 1) {
							if (tab[i - 1][j - 2].equals("X") || tab[i - 1][j - 2].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (i < 6 && j < 6) {
							if (tab[i + 2][j + 1].equals("X") || tab[i + 2][j + 1].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (i < 6 && j > 0) {
							if (tab[i + 2][j - 1].equals("X") || tab[i + 2][j - 1].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (i < 7 && j > 1) {
							if (tab[i + 1][j - 2].equals("X") || tab[i + 1][j - 2].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (i < 7 && j < 6) {
							if (tab[i + 1][j + 2].equals("X") || tab[i + 1][j + 2].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						} else {
							r = "NAO XEQUE";
						}
						break;
					case "k":
						if (i > 0) {
							if (tab[i - 1][j].equals("X") || tab[i - 1][j].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (i > 0 && j < 7) {
							if (tab[i - 1][j + 1].equals("X") || tab[i - 1][j + 1].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (j < 7) {
							if (tab[i][j + 1].equals("X") || tab[i][j + 1].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (i < 7 && j < 7) {
							if (tab[i + 1][j + 1].equals("X") || tab[i + 1][j + 1].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (i < 7) {
							if (tab[i + 1][j].equals("X") || tab[i + 1][j].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (i < 7 && j > 0) {
							if (tab[i + 1][j - 1].equals("X") || tab[i + 1][j - 1].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (j > 0) {
							if (tab[i][j - 1].equals("X") || tab[i][j - 1].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						}
						if (i > 0 && j > 0) {
							if (tab[i - 1][j - 1].equals("X") || tab[i - 1][j - 1].equals("K")) {
								r = "XEQUE";
								xm++;

							}
						} else {
							r = "NAO XEQUE";
						}
						break;
					}
				}
			}
			if (xm == contPosicoes + 1 || xm >= contPosicoes + 1) {
				r = "XEQUE-MATE";
			} else {
				r = "XEQUE";
			}
		}
		System.out.println(r);
	   
  }
}
