				case "p":
					if (i > 0 && i < 7 && j > 0 && j < 7) {
						if (tab[i + 1][j - 1].equals("K")) {
							r = "XEQUE";
							break;
						} else if (tab[i + 1][j + 1].equals("K")) {
							r = "XEQUE";
							break;
						}
