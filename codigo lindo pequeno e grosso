			for (int j = 0; j < tab.length; j++) {
				tab[i][j] = in.next();
			}
		}
		for (int i = 0; i < tab.length && r != "XEQUE"; i++) {
			for (int j = 0; j < tab.length && r != "XEQUE"; j++) {
